<!DOCTYPE html>
<html>
<head>
  <title>PgSearch</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header>
     <% if @current_user %>
       <%= link_to 'Sign Out', sign_out_path, method: :delete %>
     <% end %>
   </header>
<%= yield %>

</body>
</html>
